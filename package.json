{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:daon/tic-tac-toe.git",
    "type": "git"
  },
  "scripts": {
    "clean": "rimraf build",
    "test": "mocha \"src/**/*.test.js\" --require babel-register --require test/setup.js",
    "prestart": "yarn run test",
    "start": "webpack-dev-server --progress --profile --watch --open",
    "prebuild": "yarn run test",
    "build": "webpack --progress --profile --env=PROD",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "postdeploy": "yarn run clean"
  },
  "author": "Dan Andersson <dan.andersson@chimpiq.com> (https://chimpiq.com/)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0-rc.1",
    "webpack-dev-server": "2.2.0-rc.0"
  }
}
